{
  "name": "web2app-converter",
  "version": "1.0.0",
  "description": "Convert your HTML/CSS/JS website to Android APK using Capacitor",
  "main": "index.js",
  "scripts": {
    "buildapk": "node scripts/build-apk.js",
    "setup": "node scripts/setup.js",
    "build": "npm run setup && npm run buildapk",
    "dev": "npx cap run android",
    "sync": "npx cap sync",
    "open": "npx cap open android",
    "github-setup": "node scripts/github-setup.js",
    "update-config": "node scripts/update-config.js",
    "web2apk": "node web2apk.js"
  },
  "bin": {
    "web2apk": "./web2apk.js"
  },
  "keywords": [
    "capacitor",
    "android",
    "apk",
    "web-to-app",
    "html",
    "css",
    "javascript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@capacitor/cli": "^5.5.1",
    "@capacitor/android": "^5.5.1",
    "@capacitor/core": "^5.5.1",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "fs-extra": "^11.1.1",
    "chalk": "^4.1.2",
    "ora": "^5.4.1"
  }
}

